function displayDate()
    {
      document.getElementById("demo").innerHTML=Date();
    }

    function createTab()
    {
        alert(3);
        // Statement 1
        document.getElementById("demo").innerHTML=Date();
        chrome.tabs.create({url:"http://www.google.com"});
        // Statement 2
        // chrome.tabs.update({url:"http://en.wikipedia.org"});
        alert(4);

    }

    //
    document.addEventListener('DOMContentLoaded', function () {
        var test = document.createTextNode("New paragraph");
        //chrome.browserAction.onClicked.addListener(createTab);
        document.body.appendChild(test);
    });

---------------------------------------

<!DOCTYPE html>
<html>
  <head>
    <title>Sessions Popup</title>
    <style>
      body {
        width: 400px;
        overflow-x: hidden;
      }
    </style>

    <script>
    function displayDate()
    {
      document.getElementById("demo").innerHTML=Date();
    }

    function createTab()
    {
        alert(3);
        // Statement 1
        document.getElementById("demo").innerHTML=Date();
        chrome.tabs.create({url:"http://www.google.com"});
        // Statement 2
        // chrome.tabs.update({url:"http://en.wikipedia.org"});
        alert(4);

    }

    //
    document.addEventListener('DOMContentLoaded', function () {
        var test = document.createTextNode("New paragraph");
        //chrome.browserAction.onClicked.addListener(createTab);
        document.body.appendChild(test);
    });
    </script>
  </head>
  <body>
    Sessions
    <hr>    
    Button: 
    <button id="add_text" type="button">Button</button>
    <button type="button" onclick="createTab()">Open Tab</button>

    <p id="demo">Date Appears here</p>

  </body>
</html>

